{
  "name": "tmp-ts-backend",
  "version": "0.0.0",
  "scripts": {
    "devs": "npx prisma generate && npm run migrate_dev && npm run seeder && npm run server",
    "server": "nodemon -r tsconfig-paths/register --ignore './data/' src/app_server/main.ts",
    "daemon": "nodemon -r tsconfig-paths/register --ignore './data/' src/app_daemon/main.ts",
    "migrate_dev": "npx prisma migrate dev",
    "migrate_prod": "npx prisma generate && npx prisma migrate deploy",
    "seeder": "ts-node -r tsconfig-paths/register cmd.ts db/seeder",
    "traefik_generate": "npx ts-node -r tsconfig-paths/register cmd.ts traefik/config_generator",
    "db_clear": "ts-node -r tsconfig-paths/register cmd.ts db/clear",
    "create_test_data": "ts-node -r tsconfig-paths/register cmd.ts db/create_test_data",
    "test": "jest -i",
    "pretty": "prettier --write \"./**/*.{js,ts,jsx,json}\"",
    "build:dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack"
  },
  "private": true,
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "aws-sdk": "^2.1013.0",
    "axios": "^0.23.0",
    "base-x": "^3.0.9",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "file-type": "^16.5.3",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.0.0",
    "handy-redis": "^2.3.1",
    "hasha": "^5.2.2",
    "html-to-text": "^5.1.1",
    "jimp": "^0.16.1",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moralis": "^0.0.132",
    "multer": "^1.4.3",
    "multihashes": "^4.0.3",
    "nodemailer": "^6.7.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.29.2",
    "validator": "^13.6.0",
    "web3": "=1.3.6",
    "web3-eth-contract": "=1.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.35",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.12",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/multer": "^1.4.5",
    "@types/node": "~14.14.35",
    "@types/nodemailer": "^6.4.1",
    "@types/pug": "^2.0.4",
    "@types/request-promise": "^4.1.47",
    "@types/sharp": "^0.28.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@types/web3": "^1.2.2",
    "jest": "^27.0.4",
    "nodemon": "^2.0.14",
    "prisma": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
